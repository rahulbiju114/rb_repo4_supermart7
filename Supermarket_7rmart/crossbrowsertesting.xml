<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CrossBrowsingTesting">
	<listeners>
		<listener class-name="generateTestReport.Listners"></listener>
	</listeners>
	<test name="Chrome Test">
		<parameter name="Browser" value="Chrome"></parameter>

		<classes>
		<class name="testscripts.SubCategory_TestCase">
				<methods>
					<include name="enterValues"></include>
				</methods>
			</class>
			
			
		</classes>
	</test>

	<test name="Edge Test">
		<parameter name="Browser" value="Edge"></parameter>

		<classes>
			<class name="testscripts.Category_TestCase">
				<methods>
					<include name="enterValues"></include>
				</methods>
			</class>
		</classes>
	</test>


</suite> 